<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="2022-11-25-changes-filling-table-user_statistic" author="m.surmach">
        <preConditions onFail="MARK_RAN">
                <tableExists tableName="user_statistic"/>
        </preConditions>
        <insert tableName="user_statistic">
            <column name="telegram_user_id" valueComputed="(SELECT id FROM telegram_user)"/>
        </insert>
    </changeSet>
</databaseChangeLog>